python script/average.py data/train01.jsonl data/normal01.jsonl data/average01.jsonl
python script/average.py data/train02.jsonl data/normal02.jsonl data/average02.jsonl
python script/average.py data/train03.jsonl data/normal03.jsonl data/average03.jsonl
python script/average.py data/train04.jsonl data/normal04.jsonl data/average04.jsonl
python script/average.py data/train05.jsonl data/normal05.jsonl data/average05.jsonl
echo The processing of averaging and normalization has ended.
python script/variance.py data/average01.jsonl data/normal01.jsonl data/variance01.jsonl
python script/variance.py data/average02.jsonl data/normal02.jsonl data/variance02.jsonl
python script/variance.py data/average03.jsonl data/normal03.jsonl data/variance03.jsonl
python script/variance.py data/average04.jsonl data/normal04.jsonl data/variance04.jsonl
python script/variance.py data/average05.jsonl data/normal05.jsonl data/variance05.jsonl
echo Variance calculation is over.
python script/weighting.py data/variance01.jsonl data/average01.jsonl data/test.jsonl
python script/weighting.py data/variance02.jsonl data/average02.jsonl data/test.jsonl
python script/weighting.py data/variance03.jsonl data/average03.jsonl data/test.jsonl
python script/weighting.py data/variance04.jsonl data/average04.jsonl data/test.jsonl
python script/weighting.py data/variance05.jsonl data/average05.jsonl data/test.jsonl
